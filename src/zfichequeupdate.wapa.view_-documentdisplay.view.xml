<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" controllerName="UpdateCheque.controller.DocumentDisplay"                                                                                                                          
	xmlns:html="http://www.w3.org/1999/xhtml">                                                                                                                                                                                                                    
	<App>                                                                                                                                                                                                                                                         
		<pages>                                                                                                                                                                                                                                                      
			<Page title="Document List">                                                                                                                                                                                                                                
				<content>                                                                                                                                                                                                                                                  
					<Table id="idDocumentTable" mode="MultiSelect" items="{foregroundData>/data}">                                                                                                                                                                            
						<columns>                                                                                                                                                                                                                                                
							<Column>                                                                                                                                                                                                                                                
								<header>                                                                                                                                                                                                                                               
									<Text text="Document Number"></Text>                                                                                                                                                                                                                  
								</header>                                                                                                                                                                                                                                              
							</Column>                                                                                                                                                                                                                                               
							<Column>                                                                                                                                                                                                                                                
								<header>                                                                                                                                                                                                                                               
									<Text text="Company Code"></Text>                                                                                                                                                                                                                     
								</header>                                                                                                                                                                                                                                              
							</Column>                                                                                                                                                                                                                                               
							<Column>                                                                                                                                                                                                                                                
								<header>                                                                                                                                                                                                                                               
									<Text text="Fiscal Year"></Text>                                                                                                                                                                                                                      
								</header>                                                                                                                                                                                                                                              
							</Column>                                                                                                                                                                                                                                               
							<Column>                                                                                                                                                                                                                                                
								<header>                                                                                                                                                                                                                                               
									<Text text="Assignment Number"></Text>                                                                                                                                                                                                                
								</header>                                                                                                                                                                                                                                              
							</Column>                                                                                                                                                                                                                                               
						</columns>                                                                                                                                                                                                                                               
						<items>                                                                                                                                                                                                                                                  
							<ColumnListItem>                                                                                                                                                                                                                                        
								<cells>                                                                                                                                                                                                                                                
									<Text text="{foregroundData>Documentnumber}"></Text>                                                                                                                                                                                                  
									<Text text="{foregroundData>Companycode}"></Text>                                                                                                                                                                                                     
									<Text text="{foregroundData>Fiscalyear}"></Text>                                                                                                                                                                                                      
									<Text text="{foregroundData>Assignment}"></Text>                                                                                                                                                                                                      
								</cells>                                                                                                                                                                                                                                               
							</ColumnListItem>                                                                                                                                                                                                                                       
						</items>                                                                                                                                                                                                                                                 
					</Table>                                                                                                                                                                                                                                                  
				</content>                                                                                                                                                                                                                                                 
				<footer>                                                                                                                                                                                                                                                   
					<Bar>                                                                                                                                                                                                                                                     
						<contentRight>                                                                                                                                                                                                                                           
							<Button text="Update" icon="sap-icon://edit" press="handleDocUpdate"></Button>                                                                                                                                                                          
						</contentRight>                                                                                                                                                                                                                                          
						<contentLeft>                                                                                                                                                                                                                                            
							<Button icon="sap-icon://message-popup" tooltip="Messages" press="handleMessagesPress"></Button>                                                                                                                                                        
							<Button icon="sap-icon://reset" tooltip="Clear Msgs" press="clearMessages"></Button>                                                                                                                                                                    
						</contentLeft>                                                                                                                                                                                                                                           
					</Bar>                                                                                                                                                                                                                                                    
				</footer>                                                                                                                                                                                                                                                  
			</Page>                                                                                                                                                                                                                                                     
		</pages>                                                                                                                                                                                                                                                     
	</App>                                                                                                                                                                                                                                                        
</mvc:View>                                                                                                                                                                                                                                                    