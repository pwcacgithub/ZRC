<mvc:View controllerName="ZInvoiceListing.controller.main" xmlns:html="http://www.w3.org/1999/xhtml" xmlns:mvc="sap.ui.core.mvc"                                                                                                                               
	xmlns:core="sap.ui.core" displayBlock="true" xmlns="sap.m" xmlns:smartFilterBar="sap.ui.comp.smartfilterbar"                                                                                                                                                  
	xmlns:smartTable="sap.ui.comp.smarttable">                                                                                                                                                                                                                    
	<App>                                                                                                                                                                                                                                                         
		<pages>                                                                                                                                                                                                                                                      
			<Page title="{i18n>title}">                                                                                                                                                                                                                                 
				<content>                                                                                                                                                                                                                                                  
					<!-- Filter Bar -->                                                                                                                                                                                                                                       
					<smartFilterBar:SmartFilterBar id="smartFilterBar" class="sapUiSizeCompact" entitySet="InvoiceListingSet">                                                                                                                                                
						<smartFilterBar:controlConfiguration>                                                                                                                                                                                                                    
							<smartFilterBar:ControlConfiguration groupId="_BASIC" hasValueHelpDialog="true" index="1"                                                                                                                                                               
								key="Werks" label="{i18n>plant}" preventInitialDataFetchInValueHelpDialog="false" visible="true"></smartFilterBar:ControlConfiguration>                                                                                                                
						</smartFilterBar:controlConfiguration>                                                                                                                                                                                                                   
						<smartFilterBar:controlConfiguration>                                                                                                                                                                                                                    
							<smartFilterBar:ControlConfiguration id="idDate" mandatory="mandatory" controlType="date" groupId="_BASIC" hasValueHelpDialog="false"                                                                                                                   
								index="2" key="Bldat" label="{i18n>invoice_date}" preventInitialDataFetchInValueHelpDialog="false" visible="true" filterType="interval">                                                                                                               
								<!-- <smartFilterBar:customControl>  -->                                                                                                                                                                                                               
								<!-- <DatePicker id="idFiscalDate" displayFormat="short"/>  -->                                                                                                                                                                                        
								<!-- <DateRangeSelection id="timeRangeFilter" change="checkDateNow"/>  -->                                                                                                                                                                             
								<!-- </smartFilterBar:customControl>  -->                                                                                                                                                                                                              
							</smartFilterBar:ControlConfiguration>                                                                                                                                                                                                                  
						</smartFilterBar:controlConfiguration>                                                                                                                                                                                                                   
						<smartFilterBar:controlConfiguration>                                                                                                                                                                                                                    
							<smartFilterBar:ControlConfiguration controlType="auto" groupId="_BASIC" hasValueHelpDialog="true" index="3" key="Lifnr"                                                                                                                                
								label="{i18n>vendor}" preventInitialDataFetchInValueHelpDialog="false" visible="true"></smartFilterBar:ControlConfiguration>                                                                                                                           
						</smartFilterBar:controlConfiguration>                                                                                                                                                                                                                   
						<smartFilterBar:controlConfiguration>                                                                                                                                                                                                                    
							<smartFilterBar:ControlConfiguration controlType="auto" groupId="_BASIC" hasValueHelpDialog="true" index="4" key="Ebeln"                                                                                                                                
								label="{i18n>purchase_order}" preventInitialDataFetchInValueHelpDialog="false" visible="true"></smartFilterBar:ControlConfiguration>                                                                                                                   
						</smartFilterBar:controlConfiguration>                                                                                                                                                                                                                   
						<smartFilterBar:controlConfiguration>                                                                                                                                                                                                                    
							<smartFilterBar:ControlConfiguration controlType="auto" groupId="_BASIC" hasValueHelpDialog="true" index="5" key="Anln1"                                                                                                                                
								label="{i18n>asset}" preventInitialDataFetchInValueHelpDialog="false" visible="true"></smartFilterBar:ControlConfiguration>                                                                                                                            
						</smartFilterBar:controlConfiguration>                                                                                                                                                                                                                   
						<smartFilterBar:controlConfiguration>                                                                                                                                                                                                                    
							<smartFilterBar:ControlConfiguration controlType="auto" groupId="_BASIC" hasValueHelpDialog="true" index="6" key="Statustext"                                                                                                                           
								label="statustext" preventInitialDataFetchInValueHelpDialog="true" visible="true"></smartFilterBar:ControlConfiguration>                                                                                                                               
						</smartFilterBar:controlConfiguration>                                                                                                                                                                                                                   
						<smartFilterBar:controlConfiguration>                                                                                                                                                                                                                    
							<smartFilterBar:ControlConfiguration id="idStatus" key="Rbstat" hasValueHelpDialog="false"&nbsp;                                                                                                                                                        
							index="7" label="{i18n>status}" groupId="_BASIC"                                                                                                                                                                                                        
								preventInitialDataFetchInValueHelpDialog="false" visible="true">                                                                                                                                                                                       
								<!--<smartFilterBar:customControl>-->                                                                                                                                                                                                                  
								<!--	<Select id="foo" customData:hasValue="true">-->                                                                                                                                                                                                   
								<!--		<core:Item key="P" text="PAID"/>-->                                                                                                                                                                                                              
								<!--		<core:Item key="N" text="NOT PAID"/>-->                                                                                                                                                                                                          
								<!--	</Select>-->                                                                                                                                                                                                                                      
								<!--</smartFilterBar:customControl>-->                                                                                                                                                                                                                 
								<smartFilterBar:customControl>                                                                                                                                                                                                                         
									<ComboBox id="idCombo">                                                                                                                                                                                                                               
										<core:Item id="c1" key="P" text="PAID"></core:Item>                                                                                                                                                                                                  
										<core:Item id="c2" key="N" text="NOT PAID"></core:Item>                                                                                                                                                                                              
									</ComboBox>                                                                                                                                                                                                                                           
								</smartFilterBar:customControl>                                                                                                                                                                                                                        
							</smartFilterBar:ControlConfiguration>                                                                                                                                                                                                                  
						</smartFilterBar:controlConfiguration>                                                                                                                                                                                                                   
					</smartFilterBar:SmartFilterBar>                                                                                                                                                                                                                          
					<!-- Smart Table -->                                                                                                                                                                                                                                      
					<smartTable:SmartTable id="smartTable" entitySet="InvoiceListingSet" smartFilterId="smartFilterBar" tableType="ResponsiveTable"                                                                                                                           
						useExportToExcel="true" useVariantManagement="false" useTablePersonalisation="true" header="Invoice Listing Data" showRowCount="true">                                                                                                                   
						<smartTable:layoutData>                                                                                                                                                                                                                                  
							<FlexItemData growFactor="1" baseSize="0%"/>                                                                                                                                                                                                            
						</smartTable:layoutData>                                                                                                                                                                                                                                 
					</smartTable:SmartTable>                                                                                                                                                                                                                                  
				</content>                                                                                                                                                                                                                                                 
				<footer>                                                                                                                                                                                                                                                   
					<Bar></Bar>                                                                                                                                                                                                                                               
				</footer>                                                                                                                                                                                                                                                  
			</Page>                                                                                                                                                                                                                                                     
		</pages>                                                                                                                                                                                                                                                     
	</App>                                                                                                                                                                                                                                                        
</mvc:View>                                                                                                                                                                                                                                                    