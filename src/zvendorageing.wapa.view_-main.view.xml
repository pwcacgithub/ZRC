<mvc:View controllerName="vendorageing.controller.main" xmlns:html="http://www.w3.org/1999/xhtml" xmlns:mvc="sap.ui.core.mvc"                                                                                                                                  
	displayBlock="true" xmlns="sap.m" xmlns:smartFilterBar="sap.ui.comp.smartfilterbar" xmlns:smartTable="sap.ui.comp.smarttable">                                                                                                                                
	<App>                                                                                                                                                                                                                                                         
		<pages>                                                                                                                                                                                                                                                      
			<Page title="{i18n>title}">                                                                                                                                                                                                                                 
				<content>                                                                                                                                                                                                                                                  
					<!-- Filter Bar -->                                                                                                                                                                                                                                       
					<smartFilterBar:SmartFilterBar id="smartFilterBar" class="sapUiSizeCompact" entitySet="VendorAgeingSet">                                                                                                                                                  
						<smartFilterBar:controlConfiguration>                                                                                                                                                                                                                    
							<smartFilterBar:ControlConfiguration controlType="auto" groupId="_BASIC" hasValueHelpDialog="true" index="1" key="Lifnr"                                                                                                                                
								label="{i18n>vendor_id}" preventInitialDataFetchInValueHelpDialog="false" visible="true"></smartFilterBar:ControlConfiguration>                                                                                                                        
						</smartFilterBar:controlConfiguration>                                                                                                                                                                                                                   
						<smartFilterBar:controlConfiguration>                                                                                                                                                                                                                    
							<smartFilterBar:ControlConfiguration mandatory="mandatory" controlType="auto" groupId="_BASIC" hasValueHelpDialog="true" index="2"                                                                                                                      
								key="Bukrs" label="{i18n>company_code}" preventInitialDataFetchInValueHelpDialog="false" visible="true"></smartFilterBar:ControlConfiguration>                                                                                                         
						</smartFilterBar:controlConfiguration>                                                                                                                                                                                                                   
						<smartFilterBar:controlConfiguration>                                                                                                                                                                                                                    
							<smartFilterBar:ControlConfiguration id="idDate" mandatory="mandatory" controlType="date" groupId="_BASIC" hasValueHelpDialog="false"                                                                                                                   
								index="3" key="Allgstid" label="{i18n>fiscal_year}" preventInitialDataFetchInValueHelpDialog="false" visible="true" filterType="single">                                                                                                               
								<!-- <smartFilterBar:customControl>  -->                                                                                                                                                                                                               
								<!-- <DatePicker id="idFiscalDate" displayFormat="short"/>  -->                                                                                                                                                                                        
								<!-- <DateRangeSelection id="timeRangeFilter" change="checkDateNow"/>  -->                                                                                                                                                                             
								<!-- </smartFilterBar:customControl>  -->                                                                                                                                                                                                              
							</smartFilterBar:ControlConfiguration>                                                                                                                                                                                                                  
						</smartFilterBar:controlConfiguration>                                                                                                                                                                                                                   
					</smartFilterBar:SmartFilterBar>                                                                                                                                                                                                                          
					<IconTabBar>                                                                                                                                                                                                                                              
						<items>                                                                                                                                                                                                                                                  
							<IconTabFilter text="Normal Document">                                                                                                                                                                                                                  
								<content>                                                                                                                                                                                                                                              
									<!-- Smart Table -->                                                                                                                                                                                                                                  
									<smartTable:SmartTable id="smartTable" entitySet="VendorAgeingSet" smartFilterId="smartFilterBar" tableType="ResponsiveTable"                                                                                                                         
										useExportToExcel="true" useVariantManagement="false" useTablePersonalisation="true" header="Vendor Ageing Data" showRowCount="true">                                                                                                                 
										<smartTable:layoutData>                                                                                                                                                                                                                              
											<FlexItemData growFactor="1" baseSize="0%"/>                                                                                                                                                                                                        
										</smartTable:layoutData>                                                                                                                                                                                                                             
									</smartTable:SmartTable>                                                                                                                                                                                                                              
								</content>                                                                                                                                                                                                                                             
							</IconTabFilter>                                                                                                                                                                                                                                        
							<IconTabFilter text="Noted Item">                                                                                                                                                                                                                       
								<content>                                                                                                                                                                                                                                              
									<!-- Smart Table -->                                                                                                                                                                                                                                  
									<smartTable:SmartTable id="smartTableNoted" entitySet="VendorAgeingNotedSet" smartFilterId="smartFilterBar" tableType="ResponsiveTable"                                                                                                               
										useExportToExcel="true" useVariantManagement="false" useTablePersonalisation="true" header="Vendor Ageing Data" showRowCount="true">                                                                                                                 
										<smartTable:layoutData>                                                                                                                                                                                                                              
											<FlexItemData growFactor="1" baseSize="0%"/>                                                                                                                                                                                                        
										</smartTable:layoutData>                                                                                                                                                                                                                             
									</smartTable:SmartTable>                                                                                                                                                                                                                              
								</content>                                                                                                                                                                                                                                             
							</IconTabFilter>                                                                                                                                                                                                                                        
						</items>                                                                                                                                                                                                                                                 
					</IconTabBar>                                                                                                                                                                                                                                             
				</content>                                                                                                                                                                                                                                                 
				<footer>                                                                                                                                                                                                                                                   
					<Bar></Bar>                                                                                                                                                                                                                                               
				</footer>                                                                                                                                                                                                                                                  
			</Page>                                                                                                                                                                                                                                                     
		</pages>                                                                                                                                                                                                                                                     
	</App>                                                                                                                                                                                                                                                        
</mvc:View>                                                                                                                                                                                                                                                    